{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/src/server.ts" },
    {
      "src": "/swagger-ui/(.*)",
      "dest": "/dist/node_modules/swagger-ui-dist/$1"
    },
    { "src": "/api/(.*)", "dest": "/src/server.ts" },
    { "src": "/api-docs", "dest": "/src/server.ts" }
  ],

  "env": {
    "PRISMA_SKIP_GENERATE": "false"
  }
}
